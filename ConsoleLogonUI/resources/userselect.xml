<duixml>
    <stylesheets>
        <style resid="credviewsheet">
            <element background="argb(0, 0, 0, 0)" foreground="white" font="11;normal;shadow;Segoe UI" shadowintensity="75"/>
            <button foreground="white" font="11;normal;shadow;Segoe UI" shadowintensity="75" contentalign="middlecenter" margin="rect(6rp, 0rp, 6rp, 0rp)" accessible="true" accrole="43"/>
            <if class="GenericButtons">
                <button minsize="size(93rp,28rp)" padding="rect(10rp, 0rp, 10rp, 2rp)" borderthickness="rect(4rp, 3rp, 4rp, 3rp)" background="resbmp(12263,7,-1,0,0,1,1)" bordercolor="gray" borderstyle="raised"/>
                <if keyfocused="true">
                    <button background="resbmp(12259,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid"/>
                </if>
                <if mousefocused="true">
                    <button background="resbmp(12260,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid"/>
                </if>
                <if mousefocused="true" keyfocused="true" pressed="false">
                    <button background="resbmp(12261,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid"/>
                </if>
                <if pressed="true">
                    <button background="resbmp(12262,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="sunken"/>
                </if>
            </if>
        </style>
        
        <style resid="UsersStyle">
            <if visible="false">
                <element width="0rp" height="0rp" layoutpos="none" accessible="false"/>
                <button width="0rp" height="0rp" layoutpos="none" accessible="false"/>
                <if class="UserTile">
                    <button width="0rp" height="0rp" layoutpos="none" accessible="false"/>
                </if>
            </if>
            <if class="HorizontalViewSpacer">
                <element height="0rp" width="80rp"/>
            </if>
            <if class="UserTile">
                <button accessible="true" accrole="34" accstate="0" height="80rp"/>
                <if selected="true">
                    <button accstate="2"/>
                </if>
            </if>
            <if id="atom(PictureContainer)">
                <element width="72rp" height="86rp"/>
            </if>
            <if id="atom(PicturePadding)">
                <element padding="rect(3rp, 3rp, 3rp, 17rp)"/>
            </if>
            <if id="atom(Overlay)">
                <element width="86rp" height="86rp" borderthickness="rect(1rp, 1rp, 1rp, 1rp)" background="resbmp(1,7,-1,0,0,1,1)"/>
                <if HighDPI="true">
                    <element background="resbmp(1, 7,-1,0,0,1,1)"/>
                </if>
                <if selected="true">
                    <element background="resbmp(1,7,-1,0,0,1,1)"/>
                    <if HighDPI="true">
                        <element background="resbmp(1, 7,-1,0,0,1,1)"/>
                    </if>
                </if>
                <if mousefocused="true">
                    <element background="resbmp(1,7,-1,0,0,1,1)"/>
                    <if HighDPI="true">
                        <element background="resbmp(1, 7,-1,0,0,1,1)"/>
                    </if>
                </if>
                <if selected="true" mousefocused="true">
                    <element background="resbmp(1,7,-1,0,0,1,1)"/>
                    <if HighDPI="true">
                        <element background="resbmp(1, 7,-1,0,0,1,1)"/>
                    </if>
                </if>
            </if>
            <if id="atom(Picture)">
                <element width="67rp" height="67rp" accessible="true" accrole="40" accname="User Picture"/>
            </if>
            <if class="Link">
                <button font="9;normal;shadow|underline;Tahoma" shadowintensity="75" cursor="hand" contentalign="middlecenter"/>
                <if keyfocused="true">
                    <button contentalign="middlecenter|focusrect"/>
                </if>
                <if mousefocused="true">
                    <button fontstyle="underline"/>
                </if>
            </if>
            <if class="SmallText">
                <element font="9;normal;shadow;Tahoma" shadowintensity="75" contentalign="topcenter | endellipsis"/>
            </if>
            <if class="LargeText">
                <element font="9;normal;shadow;Tahoma" shadowintensity="75" contentalign="topcenter | endellipsis"/>
            </if>
            <if class="RestrictedEdit">
                <Edit width="225rp" height="22rp" font="9;normal;shadow;Tahoma" shadowintensity="75" margin="rect(1rp, 1rp, 0rp, 1rp)" padding="rect(0rp, 0rp, 0rp, 0rp)" borderthickness="rect(1rp, 4rp, 0rp, 1rp)" bordercolor="gray" accessible="true" accrole="42" background="resbmp(2, 7,-1,0,0,1,1)"/>
                <if mousefocused="true">
                    <Edit background="resbmp(2, 7,-1,0,0,1,1)" bordercolor="windowtext"/>
                </if>
                <if keyfocused="true">
                    <Edit background="resbmp(2, 7,-1,0,0,1,1)" bordercolor="windowtext"/>
                </if>
                <if enabled="false">
                    <Edit background="resbmp(2, 7,-1,0,0,1,1)"/>
                </if>
            </if>
            <if class="DUIComboBox">
                <Combobox width="225rp" height="42rp" font="9;normal;shadow;Tahoma" shadowintensity="75" margin="rect(0rp, 4rp, 0rp, 0rp)" padding="rect(0rp, 1rp, 0rp, 0rp)" accessible="true"/>
            </if>
            <if id="atom(Submit)">
                <Button background="resbmp(4,7,-1,0,0,1,1)" margin="rect(0rp, 0rp, 0rp, 0rp)" visible="false" enabled="false" borderthickness="rect(0rp, 0rp, 0rp, 0rp)" bordercolor="gray" borderstyle="raised"/>
                <if HighDPI="true">
                    <Button background="resbmp(4,7,-1,0,0,1,1)"/>
                </if>
                <if keyfocused="true">
                    <Button background="resbmp(4,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid"/>
                    <if HighDPI="true">
                        <Button background="resbmp(4,7,-1,0,0,1,1)"/>
                    </if>
                </if>
                <if mousefocused="true">
                    <Button background="resbmp(4,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid"/>
                    <if HighDPI="true">
                        <Button background="resbmp(4,7,-1,0,0,1,1)"/>
                    </if>
                </if>
                <if pressed="true">
                    <Button background="resbmp(4,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="sunken"/>
                    <if HighDPI="true">
                        <Button background="resbmp(4,7,-1,0,0,1,1)"/>
                    </if>
                </if>
            </if>
        </style>
        
        <style resid="ScrollStyle">
            <Viewer layoutpos="nineclient" margin="rect(0rp,  0rp, 12rp,  0rp)"/>
            <Scrollbar padding="rect(0, 0, 0, 0)"/>
            <if visible="false">
                <Scrollbar layoutpos="none"/>
                <RepeatButton accessible="false"/>
                <Thumb accessible="false"/>
            </if>
            <if vertical="true">
                <ScrollBar layoutpos="nineright"/>
            </if>
            <if id="atom(lineup)">
                <RepeatButton accessible="true" content="resbmp(12204, 2,-1,0,0,1,1)" contentalign="topcenter" accname="Line up" accrole="43"/>
                <if pressed="true">
                    <RepeatButton content="resbmp(12205,2,-1,0,0,1,1)"/>
                </if>
                <if mousefocused="true">
                    <RepeatButton content="resbmp(12205,2,-1,0,0,1,1)"/>
                </if>
            </if>
            <if id="atom(pageup)">
                <RepeatButton accessible="true" accname="Page up" accrole="43"/>
            </if>
            <Thumb borderthickness="rect(1, 10, 1, 10)" background="resbmp(12201, 7,-1,0,0,1,1)" content="resbmp(12212,2,-1,0,0,1,1)" contentalign="middlecenter" accessible="true" accname="Position" accrole="39"/>
            <if pressed="true">
                <Thumb background="resbmp(12203, 7,-1,0,0,1,1)" content="resbmp(12212,2,-1,0,0,1,1)"/>
            </if>
            <if mousefocused="true" pressed="false">
                <Thumb background="resbmp(12202, 7,-1,0,0,1,1)" content="resbmp(12211,2,-1,0,0,1,1)"/>
            </if>
            <if id="atom(pagedown)">
                <RepeatButton accessible="true" accname="Page down" accrole="43"/>
            </if>
            <if id="atom(linedown)">
                <RepeatButton accessible="true" content="resbmp(12207,2,-1,0,0,1,1)" contentalign="topcenter" accname="Line down" accrole="43"/>
                <if pressed="true">
                    <RepeatButton content="resbmp(12208,2,-1,0,0,1,1)"/>
                </if>
                <if mousefocused="true">
                    <RepeatButton content="resbmp(12208,2,-1,0,0,1,1)"/>
                </if>
            </if>
        </style>
        

    </stylesheets>

    <Button resid="UserTileTemplate" class="UserTile" margin="rect(0rp,10rp,0rp,10rp)" layout="flowlayout(0,2,0,2)" sheet="UsersStyle">
        <element id="atom(PictureContainer)" layout="filllayout()">
            <element layoutpos="client" layout="borderlayout()" id="atom(PicturePadding)">
                <element id="atom(Picture)" layoutpos="top"/>
            </element>
            <element id="atom(Overlay)" layoutpos="client"/>
        </element>
        <element content="Administrator" id="atom(UserText)" class="SmallText"/>
        <element layout="verticalflowlayout(0,0,2)">
            <element id="atom(SelectorFieldFrame)" margin="rect(0rp,0rp,0rp,5rp)" layout="verticalflowlayout(0,0,2)"/>
            <element layout="flowlayout(1,1,0,1)">
                <element id="atom(NonSelectorFieldsFrame)" visible="false" layout="verticalflowlayout(0,0,2)"/>
            </element>
            <element layout="borderlayout()">
                <element layoutpos="left" width="0rp" height="sysmetric(50)"/>
                <element id="atom(CapsLockWarning)" layoutpos="none" layout="flowlayout(0,2,2,2)">
                    <element contentalign="middlecenter" content="icon(84, sysmetric(49), sysmetric(50), library(imageres.dll))"/>
                    <element content="Caps Lock is on"/>
                </element>
            </element>
        </element>
        <element class="HorizontalViewSpacer"/>
    </Button>

    <element resid="Main" layout="filllayout()" accessible="true"  sheet="credviewsheet" background="argb(0,0,0,0)">
        <duiUserSelect id="atom(UserSelectFrame)" layout="filllayout()">
            <element layout="borderlayout()" padding="rect(0rp,96rp,0rp,0rp)">
                <element layoutpos="client" layout="verticalflowlayout(1,2,2,2)">
                    <element layout="filllayout()">
                        <element id="atom(Locked)" visible="false" layout="verticalflowlayout(1,2,2,2)">
                            <element id="atom(LockedMessage)"/>
                            <element id="atom(LockedSubMessage)"/>
                        </element>
                        <element id="atom(Window)" layout="borderlayout()" width="536rp" height="337rp" background="resbmp(12,7,-1,0,0,1,1)" borderthickness="rect(45rp, 140rp, 45rp, 45rp)" padding="rect(0rp, 0rp, 0rp, -40rp)" animation="rectanglev|s|fast">
                            <element id="atom(InsideWindow)" layoutpos="client" layout="filllayout()" animation="rectanglev|s|fast">
                                <element id="atom(LogonUserList)" visible="true" layout="filllayout()" animation="rectanglev|s|fast">
                                    <element id="atom(UserListTemplate)" width="400rp" height="262rp" sheet="UsersStyle" layoutpos="client" layout="filllayout()">
                                        <styledscrollviewer id="atom(UserListFrame)" animation="rectanglev|s|fast" sheet="ScrollStyle" xscrollable="false">
                                            <selector id="atom(UserList)" sheet="UsersStyle" layout="flowlayout(1,1,0,0)" padding="rect(12rp, 0, 0, 0)">
                                            </selector>
                                        </styledscrollviewer>
                                    </element>
                                </element>
                            </element>
                            <element id="atom(DialogButtonFrame)" layout="flowlayout(0,1,2,2)" sheet="credviewsheet" padding="rect(0rp, 10rp, 0rp, 0rp)" margin="rect(0rp, 30rp, 0rp, 5rp)" contentalign="MiddleCenter">

                            </element>
                        </element>
                    </element>
                </element>
            </element>



        </duiUserSelect>
    </element>

</duixml>