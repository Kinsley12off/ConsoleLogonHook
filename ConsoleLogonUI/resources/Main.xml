<duixml>
    <stylesheets>
        <style resid="LogonStyle">

            <if class="status">
                <element background="argb(0,0,0,0)" foreground="argb(255,255,255,255)" shadowintensity="75" fontsize="18pt" fontstyle="Shadow" padding="rect(24rp,0,0,100rp)"/>
            </if>


            <if id="atom(Background)">
                <element background="resbmp(113,4,-1,0,0,0,0)"/>
            </if>

            <if class="passwordBox">
                <edit width="225rp" height="25rp" font="resstr(11800)" shadowintensity="75" margin = "rect(0rp, 3rp, 0rp, 3rp)" padding="rect(0rp, 1rp, 0rp, 0rp)" borderthickness = "rect(4, 4, 4, 4)"
				bordercolor="gray" accessible="true" accrole="42" background="resbmp(11003, 7,-1,0,0,1,1)" passwordcharacter="9679"/>
            </if>

            <if class="button">
                <button background="resbmp(12282,7,-1,0,0,1,1)" width="30rp" height="30rp"/>
                <if mousefocused="true">
                    <button background="resbmp(12286,7,-1,0,0,1,1)"/>
                </if>
                <if pressed="true">
                    <button background="resbmp(12287,7,-1,0,0,1,1)"/>
                </if>
            </if>

            <if id="atom(buttonEaseOfAccess)">
                <button width="38rp" height="28rp" padding="rect(0,0,0,0)" contentalign="MiddleCenter" content="resbmp(12213,2,-1,0,0,0,1)" accname="resstr(12122)" accdesc="resstr(12122)" borderthickness="rect(4rp, 2rp, 4rp, 2rp)" background="resbmp(12263,7,-1,0,0,1,1)" bordercolor="gray" borderstyle="raised"/>
                <if keyfocused="true">
                    <button borderthickness="rect(4rp, 2rp, 4rp, 2rp)" background="resbmp(12259,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid"/>
                </if>
                <if mousefocused="true">
                    <button borderthickness="rect(4rp, 2rp, 4rp, 2rp)" background="resbmp(12260,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid"/>
                </if>
                <if keyfocused="true" mousefocused="true" pressed="false">
                    <button borderthickness="rect(4rp, 2rp, 4rp, 2rp)" background="resbmp(12261,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid"/>
                </if>
                <if pressed="true">
                    <button borderthickness="rect(4rp, 2rp, 4rp, 2rp)" background="resbmp(12262,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="sunken"/>
                </if>
            </if>

            <if id="atom(ToggleTabletKeyboard)">
                <button width="45rp" height="28rp" padding="rect(0,0,0,0)" content="resbmp(12214,2,-1,0,0,0,1)" accname="resstr(12142)" accdesc="resstr(12142)" borderthickness="rect(4rp, 2rp, 4rp, 2rp)" background="resbmp(12263,7,-1,0,0,1,1)" bordercolor="gray" borderstyle="raised"/>
                <if keyfocused="true">
                    <button borderthickness="rect(4rp, 2rp, 4rp, 2rp)" background="resbmp(12259,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid"/>
                </if>
                <if mousefocused="true">
                    <button borderthickness="rect(4rp, 2rp, 4rp, 2rp)" background="resbmp(12260,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid"/>
                </if>
                <if keyfocused="true" mousefocused="true" pressed="false">
                    <button borderthickness="rect(4rp, 2rp, 4rp, 2rp)" background="resbmp(12261,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid"/>
                </if>
                <if pressed="true">
                    <button borderthickness="rect(4rp, 2rp, 4rp, 2rp)" background="resbmp(12262,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="sunken"/>
                </if>
            </if>
            <if id="atom(ShutdownFrame)">
                <element margin="rect(6rp, 0rp, 6rp, 0rp)"/>
            </if>
            <if id="atom(Shutdown)">
                <button height="28rp" borderthickness="rect(4rp, 1rp, 4rp, 1rp)" background="resbmp(12296,7,-1,0,0,1,1)" foreground="white" contentalign="middlecenter" content="Shut down" margin="rect(0,0,0,0)" accdesc="resstr(12128)" bordercolor="white" borderstyle="raised"/>
                <if selected="true">
                    <button content="resstr(3008)" accdesc="resstr(12133)" bordercolor="windowtext" borderstyle="solid"/>
                </if>
                <if keyfocused="true">
                    <button background="resbmp(12293,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid" font="11;normal;underline|shadow;Tahoma" shadowintensity="75"/>
                </if>
                <if mousefocused="true">
                    <button background="resbmp(12294,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid" font="11;normal;underline|shadow;Tahoma" shadowintensity="75"/>
                </if>
                <if mousefocused="true" keyfocused="true" pressed="false">
                    <button background="resbmp(12292,7,-1,0,0,1,1)" borderstyle="solid" font="11;normal;underline|shadow;Tahoma" shadowintensity="75"/>
                </if>
                <if pressed="true">
                    <button background="resbmp(12295,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="sunken"/>
                </if>
            </if>
            <if id="atom(ShutdownOptions)">
                <button width="20rp" height="28rp" foreground="white" contentalign="middlecenter" background="resbmp(12302,7,-1,0,0,1,1)" borderthickness="rect(4rp, 3rp, 4rp, 3rp)" margin="rect(0,0,0,0)" bordercolor="gray" borderstyle="raised" content=">"/>
                <if keyfocused="true">
                    <button background="resbmp(12299,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid" font="11;normal;underline|shadow;Tahoma" shadowintensity="75"/>
                </if>
                <if mousefocused="true">
                    <button background="resbmp(12300,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid" font="11;normal;underline|shadow;Tahoma" shadowintensity="75"/>
                </if>
                <if mousefocused="true" keyfocused="true" pressed="false">
                    <button background="resbmp(12298,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="solid" font="11;normal;underline|shadow;Tahoma" shadowintensity="75"/>
                </if>
                <if pressed="true">
                    <button background="resbmp(12301,7,-1,0,0,1,1)" bordercolor="windowtext" borderstyle="sunken"/>
                </if>
            </if>




        </style>
    </stylesheets>


    <element resid="Main" layout="flowlayout(1,2,2,2)" sheet="LogonStyle">
        <duiBackgroundWindow width="2560rp" height="1600rp" id="atom(InsideFrame)" layout="filllayout()">
            <!--This is the footer for the branding and the options buttons such as the shutdown,ease of access and power options-->
            <element layout="borderlayout()" id="atom(Background)">
                <element id="atom(SoftKeyboardParent)" layout="borderlayout()" layoutpos="none" />
                <element id="atom(Options)" layoutpos="bottom" visible="true" enabled="true" layout="filllayout()" background="argb(0,116,184,252)" height="96rp">

                    <element id="atom(OptionsButton)" layout="borderlayout()" padding="rect(34rp, 34rp, 34rp, 34rp)">
                        <button id="atom(buttonEaseOfAccess)" tooltip="true" layoutpos="none"/>
                        <button id="atom(ToggleTabletKeyboard)" tooltip="true" layoutpos="none"/>
                        <element layout="flowlayout(0,1,2,2)" layoutpos="left">
                            <element id="atom(ShutDownFrame)" layout="flowlayout()">
                                <button id="atom(ShutDown)" accname="resstr(12128)" tooltip="true"/>
                                <button id="atom(ShutDownOptions)" accname="resstr(12131)" accdesc="resstr(12131)" tooltip="true"/>
                            </element>
                        </element>
                    </element>
                    <element layout="flowlayout(0,2,2,2)">
                        <element id="atom(BrandingImage)" class="branding" layoutpos="none"/>
                    </element>
                </element>
                <element layoutpos="auto" visible="true" enabled="true" layout="filllayout()" background="argb(0,116,184,252)" height="1504rp">
                    <element layout="filllayout()" id="atom(activePageContainer)" >
                        
                    </element>
                    <!-- this is where the backend will place in the uifiles for the active page-->>
                </element>
            </element>
        </duiBackgroundWindow>
    </element>
</duixml>